	<content><![CDATA[
ll powmod(ll a,ll b){
	if(b==0) return 1;
	if(b&1) return a*powmod(a*a%MOD,b/2)%MOD;
	else return powmod(a*a%MOD,b/2)%MOD;
}
ll inv(ll a){
	return powmod(a,MOD-2);
}
ll nck(ll n,ll k){
	if(k==0) return 1;
	return nck(n-1,k-1)*n%MOD*inv(k)%MOD;
}
ll fact(ll n){
	ll ans=n;
	while(n--) ans=ans*n%MOD;
	return ans;
}
map<ll,ll> fac(ll x){
	map<ll,ll> fac;
	for(ll p=2;p*p<=x;p++){
		while(x%p==0){
			x/=p;
			fac[p]++;
		}
	}
	if(x!=1)
		fac[x]++;
	return fac;
}
]]></content>
